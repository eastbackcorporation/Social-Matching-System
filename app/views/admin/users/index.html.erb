<div class="block">
  <div class="menu">
    <%= render "admin/sidemenu" %>
  </div>

  <div class="index_content">
    <h2>ユーザ一覧</h2>
    <table id="index_massages">
      <thead>
        <tr><th>ログイン名</th><th>氏名</th><th>メールアドレス</th><th>電話番号</th><th>リンク</th></tr>
      </thead>
      <% @admin_users.each do |user| %>
        <tr>
          <td><%= user.login %></td>
          <td><%= user.family_name+""+user.given_name %></td>
          <td><%= user.email %></td>
          <td><%= user.phone_number %></td>
          <td><%= link_to("詳細",admin_user_path(user.id)) %></td>
        </tr>
      <% end %>
    </table>
    <%= will_paginate(@admin_users,:previous_label => "前", :next_label => "次") %>
  </div>
</div>
