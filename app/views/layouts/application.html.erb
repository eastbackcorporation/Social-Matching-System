<!DOCTYPE html>
<html>
	<head>
		<title>SocialMatchingSystem-だんだんマッチング-</title>
		<%= stylesheet_link_tag    "application", :media => "all" %>
		<%= javascript_include_tag "application" %>
		<%= javascript_include_tag params[:controller] %>
		
		<%= javascript_include_tag "jquery.sheepItPlugin-1.0.0" %>
		<%= javascript_include_tag "jquery.meio.mask" %>
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
		<script src="http://ajaxzip3.googlecode.com/svn/trunk/ajaxzip3/ajaxzip3.js" charset="UTF-8"></script>
		
		<%= jqgrid_javascripts %>
		<%= jqgrid_stylesheets %>

		<%= csrf_meta_tags %>
	</head>
	<body>

		<div id="main_container">

			<div class="header">
				<div class="logo">
					<a href="#">Social Matching System - だんだんマッチング -</a>
				</div>

				<div class="right_header">
					<%= render "shared/header" %>
				</div>
			</div>

			<div class="main_content">
				<%= yield %>
				<div class="clear"></div>
			</div>

			<div class="footer">

				<div class="left_footer">
					Social Matching System | Powered by <a href="http://eastback.co.jp">EastBack</a>
				</div>
				<div class="right_footer">
					<a href="http://eastback.co.jp">logo</a>
				</div>

			</div>

		</div>

	</body>
</html>
